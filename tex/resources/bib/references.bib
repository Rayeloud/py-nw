	keywords = {Computer Science - Neural and Evolutionary Computing, Mathematics - Numerical Analysis},
	file = {Wight and Zhao - 2020 - Solving Allen-Cahn and Cahn-Hilliard Equations usi.pdf:/Users/rayaneeloudrhiri/Zotero/storage/K2IYWZXW/Wight and Zhao - 2020 - Solving Allen-Cahn and Cahn-Hilliard Equations usi.pdf:application/pdf},
