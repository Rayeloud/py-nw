@article{bartels_variational_2015,
	title = {On the variational formulation and implementation of Allen-Cahn and Cahn-Hilliard-type phase field theories},
	volume = {15},
	issn = {1617-7061},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/pamm.201510127},
	doi = {10.1002/pamm.201510127},
	abstract = {Phase field theory is a promising framework for analyzing evolving microstructures in materials. Phenomena like those related to microstructures in Ni-based superalloys, twin structures in martensites or precipitation in Al-alloys can be predicted by phase field theory. While phase transformations such as those characterizing twinning are captured by an Allen-Cahn-type approach, a Cahn-Hilliard-type formulation is used, if the respective interface motion is driven by the concentration of the species. Although the Allen-Cahn and the Cahn-Hilliard formulation are indeed different, they do share some similarities. To be more precise, a Cahn-Hilliard model is obtained by enforcing balance of mass in the Allen-Cahn approach. Within an energy-based formulation this can be implemented by adding additional energy terms to the underlying Allen-Cahn energy. Such a universal energy-based framework is elaborated in this presentation. (© 2015 Wiley-{VCH} Verlag {GmbH} \& Co. {KGaA}, Weinheim)},
	pages = {273--274},
	number = {1},
