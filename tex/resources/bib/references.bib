	author = {Zhang, Yongzheng and Ren, Huilong and Rabczuk, Timon},
	date = {2022-03-31},
	file = {Full Text:/Users/rayaneeloudrhiri/Zotero/storage/8R2CI9LA/Zhang et al. - 2022 - Nonlocal Operator Method for Solving Partial Diffe.pdf:application/pdf},
}

